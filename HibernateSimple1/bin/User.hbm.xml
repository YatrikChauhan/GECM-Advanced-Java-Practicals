<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.yatrik.User" table="Employee">
		
		<meta attribute="class-description">This class contains Employee details.</meta>
		<id name="id" type="int" column="id">
			<generator class="native"></generator>
		</id>
		<property name="fname" column="fname" type="string"></property>
		<property name="lname" column="lname" type="string"></property>
	</class>
</hibernate-mapping>